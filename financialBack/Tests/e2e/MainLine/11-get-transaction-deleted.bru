get {
  url: http://localhost:{{port}}/Transaction/{{transaction2Id}}
  body: none
  auth: none
}

assert {
  res.status: eq 404
}

script:post-response {
  console.log(res.body);
  if (res.status !== 404) {
    console.error("‚ùå [GET DELETED TRANSACTION] Expected 404 but got:", res.status);
