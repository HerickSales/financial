  } else {
    console.log("‚úÖ [GET DELETED TRANSACTION] Correctly received 404 for deleted transaction.");
    console.log("Transaction ID confirmed deleted:", bru.getVar("transaction2Id"));
    console.log("üéâ [E2E TEST] All tests completed successfully!");
  }
}


